  gstreamer0.10-plugins-gnome_vfs
  gstreamer0.10-plugins-mad
  gstreamer0.10-plugins-musepack
  gstreamer0.10-plugins-ogg
  gstreamer0.10-plugins-oss
  gstreamer0.10-plugins-spc
  gstreamer0.10-plugins-speex
  gstreamer0.10-plugins-vorbis
  gstreamer0.10-plugins-wavpack
  libgcc1-4.3.4-1
  libglib2.0_0-2.20.5-1
  libgtk2.0_0-2.16.6-1
  libmodplug0-0.8.7-1
  libX11_6-1.2.2-2
  python-2.5.4-10
  python-dbus-0.83.0-10
  python-feedparser-4.1-2
